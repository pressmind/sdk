{
    "name": "pressmind/sdk",
    "description": "Core functionality for the pressmind Web-Core SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Graute",
            "email": "m.graute@team-braeutigam.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-bcmath": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-readline": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-zlib": "*",
        "ext-fileinfo": "*",
        "ext-redis": "*",
        "ext-imagick": "*",
        "gettext/gettext": "*",
        "jc21/clitable": "*",
        "opensearch-project/opensearch-php": "^2.4.3",
        "symfony/http-client": "^7.3",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Pressmind\\": "src/Pressmind/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
