              },
              {
                "class": "\\Pressmind\\Image\\Filter\\InstaFilter",
                "params": {
                  "preset": "vintage",
                  "intensity": 0.5
                }
              },
              {
                "class": "\\Pressmind\\Image\\Filter\\GrayscaleFilter",
                "params": {}
              }
            ]
          }
        }
